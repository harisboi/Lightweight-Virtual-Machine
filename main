// Include the "stack-vm.h" header file which presumably defines the StackVM class
#include "stack-vm.h"

// Define the main function
int main(){
    
    // Create an instance of the StackVM class called "vm"
    StackVM vm;

    // Create a vector of integers containing the program to be executed
    std::vector<i32> prog{3,4,0x40000001,0x40000000};

    // Load the program into the StackVM's memory
    vm.loadProgram(prog);

    // Execute the loaded program
    vm.run();

    // Indicate successful completion of the program
    return 0;
}

